[user]
  name = Dan Shearmur
  email = dan.shearmur@semantico.com
[color]
  ui = auto
[core]
  whitespace = tab-in-indent,tabwidth=2
  fsyncobjectfiles = true
  preloadindex = true
[branch]
  autosetupmerge = always
[diff]
  mnemonicprefix = true
  renames = copy
[pack]
  threads = 0
[rerere]
  enabled = 1
[branch "master"]
  remote = origin
  merge = refs/heads/master
[url "git@github.com:"]
  insteadOf = "gh:"
  pushInsteadOf = "github:"
  pushInsteadOf = "git://github.com/"
  pushInsteadOf = "https://github.com/"
  pullInsteadOf = "github:"
  pullInsteadOf = "git://github.com/"
  pullInsteadOf = "https://github.com/"
[merge]
  log = true
  tool = vimdiff
[url "git@gitlab.semantico.net:"]
	insteadof = s:
